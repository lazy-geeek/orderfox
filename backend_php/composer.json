{
    "name": "orderfox/backend-php",
    "description": "OrderFox PHP Backend - Cryptocurrency trading application backend",
    "type": "project",
    "require": {
        "php": "^8.1",
        "react/react": "^1.4",
        "react/socket": "^1.16",
        "ratchet/pawl": "^0.4.3",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.9",
        "ccxt/ccxt": "^4.4",
        "cboden/ratchet": "^0.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "OrderFox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OrderFox\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
